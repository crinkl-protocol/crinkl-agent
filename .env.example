# Crinkl API key — get one from https://app.crinkl.xyz/profile/agent-keys
CRINKL_API_KEY=crk_your_api_key_here

# Crinkl API base URL (default: https://api.crinkl.xyz)
# CRINKL_API_URL=https://api.crinkl.xyz

# Google OAuth credentials — create at https://console.cloud.google.com/apis/credentials
GMAIL_CLIENT_ID=your_client_id.apps.googleusercontent.com
GMAIL_CLIENT_SECRET=your_client_secret

# Max email age in days (default: 14, server-enforced)
# MAX_EMAIL_AGE_DAYS=14
